<html>
<head>
  <%- include('header') %>
</head>
<body>
  <%- include('navbar') %>
  <div class="container">
    <h2>Data per URL</h2>
    <table id="URLTable" class="table">
      <thead>
        <tr>
          <th>URL</th>
          <th>Type</th>
          <th>Data</th>
          <th>Last <%= NUM_OF_LAST_RUNS %> runs</th>
          <th>Average of last <%= NUM_OF_LAST_RUNS %> durations</th>
          <th>Average of last <%= NUM_OF_LAST_RUNS %> sizes</th>
        </tr>
      </thead>
      <tbody>
        <% if (urlRecords) {
          for (var i=0; i<urlRecords.length; i++) { %>
            <tr>
              <td><%= urlRecords[i]['url'] %></td>
              <td><%= urlRecords[i]['type'] %></td>
              <td><%= urlRecords[i]['data'] %></td>
              <td><%= JSON.stringify(urlRecords[i]['lastDurations']) %></td>
              <td><%= urlRecords[i]['lastDurationsAverage'] %></td>
              <td><%= urlRecords[i]['lastSizesAverage'] %></td>
            </tr>
          <% }
        } %>
      </tbody>
    </table>
  </div>
</body>
</html>
